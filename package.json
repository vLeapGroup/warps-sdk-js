{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "packages:publish": "turbo run build lint test && changeset publish",
    "typegen": "ts-json-schema-generator --path 'packages/core/src/types/index.ts' --type 'Warp' --tsconfig 'tsconfig.typegen.json' --out 'warp-schema.json'",
    "play": "turbo run build && ts-node --esm packages/playground/playground.ts"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "eslint-config-custom": "*",
    "prettier": "^3.6.2",
    "ts-json-schema-generator": "^2.4.0",
    "ts-node": "^10.9.2",
    "turbo": "^2.5.6"
  },
  "name": "warp-sdk-js",
  "packageManager": "npm@11.6.0",
  "workspaces": [
    "packages/*"
  ]
}
